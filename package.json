{
  "name": "gatsby-source-prismic",
  "version": "3.0.0-alpha.3",
  "description": "Gatsby source plugin for building websites using prismic.io as a data source",
  "main": "index.cjs.js",
  "module": "index.esm.js",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build:watch": "yarn build --watch",
    "format": "prettier --write 'src/**/*.js' 'rollup.config.js' 'docs/**/*.md'",
    "prebuild": "rimraf dist",
    "prepare": "npm run build",
    "prepublish": "yarn build",
    "release": "standard-version",
    "test": "jest",
    "test:watch": "yarn test --watch"
  },
  "homepage": "https://github.com/angeloashmore/gatsby-source-prismic",
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloashmore/gatsby-source-prismic.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "prismic"
  ],
  "author": "Angelo Ashmore <angelo.ashmore@walltowall.com>",
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^15.0.3",
    "es-cookie": "^1.2.0",
    "gatsby-source-filesystem": "^2.0.33",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "pascalcase": "^0.1.1",
    "prismic-dom": "^2.1.0",
    "prismic-javascript": "^2.0.3",
    "ramda": "^0.26.1",
    "ramda-adjunct": "^2.17.0",
    "traverse": "^0.6.6",
    "uuid": "^3.3.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.0",
    "react": "^16.8.6",
    "react-hooks-testing-library": "^0.5.0",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "rollup": "^1.11.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-string": "^3.0.0",
    "standard-version": "^6.0.1"
  },
  "peerDependencies": {
    "gatsby": ">=2.3.0",
    "react": ">=16.8"
  }
}
