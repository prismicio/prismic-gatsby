{
  "name": "gatsby-source-prismic",
  "version": "3.0.0-imgix.3",
  "description": "Gatsby source plugin for building websites using prismic.io as a data source",
  "main": "index.cjs.js",
  "module": "index.esm.js",
  "sideEffects": false,
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build:watch": "npm run build -- --watch",
    "format": "prettier --write 'src/**/*.js' 'rollup.config.js' 'docs/**/*.md'",
    "prebuild": "rimraf dist",
    "prepare": "npm run build",
    "release": "standard-version",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "homepage": "https://github.com/angeloashmore/gatsby-source-prismic",
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloashmore/gatsby-source-prismic.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "prismic"
  ],
  "author": "Angelo Ashmore <angelo.ashmore@walltowall.com>",
  "license": "MIT",
  "dependencies": {
    "camelcase": "^5.3.1",
    "compose-tiny": "^1.1.3",
    "es-cookie": "^1.2.0",
    "gatsby-source-filesystem": "^2.0.33",
    "lodash.clonedeep": "^4.5.0",
    "lodash.mergewith": "^4.6.2",
    "md5": "^2.2.1",
    "pascalcase": "^0.1.1",
    "prismic-dom": "^2.1.0",
    "prismic-javascript": "^2.0.3",
    "query-string": "^6.8.1",
    "ramda": "^0.26.1",
    "ramda-adjunct": "^2.17.0",
    "traverse": "^0.6.6",
    "uuid": "^3.3.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@testing-library/react-hooks": "^1.1.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-plugin-ramda": "^2.0.0",
    "eslint": "^6.1.0",
    "eslint-plugin-jest": "^22.15.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^5.1.1",
    "standard-version": "^6.0.1"
  },
  "peerDependencies": {
    "gatsby": ">=2.3",
    "react": ">=16.8"
  },
  "browserslist": ">1%, not ie 11, not op_mini all",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
