{
	"name": "gatsby-source-prismic",
	"version": "6.0.1",
	"description": "Gatsby source plugin for building websites using Prismic as a data source",
	"keywords": [
		"gatsby",
		"gatsby-plugin",
		"gatsby-source-plugin",
		"prismic"
	],
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio-community/prismic-gatsby-early-access.git"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"sideEffects": false,
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./gatsby-node": "./dist/gatsby-node.cjs",
		"./package.json": "./package.json"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"src",
		"gatsby-node.js"
	],
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"format": "prettier --write .",
		"lint": "eslint --ext .js,.ts .",
		"prepare": "npm run build",
		"test": "npm run lint && npm run types && npm run unit && npm run build",
		"types": "vitest typecheck --run",
		"types:watch": "vitest typecheck",
		"unit": "vitest run --coverage",
		"unit:watch": "vitest watch"
	},
	"dependencies": {
		"@prismicio/client": "^7.1.0",
		"@prismicio/custom-types-client": "^1.1.0",
		"camel-case": "^4.1.2",
		"gatsby-plugin-utils": "^4.11.0",
		"gatsby-source-filesystem": "^5.11.0",
		"imgix-url-builder": "^0.0.4",
		"imgix-url-params": "^11.15.0",
		"node-fetch": "^3.3.1",
		"p-queue": "^7.3.4",
		"pascal-case": "^3.1.2"
	},
	"devDependencies": {
		"@prismicio/mock": "^0.3.1",
		"@trivago/prettier-plugin-sort-imports": "^4.1.1",
		"@types/node": "^20.3.1",
		"@types/node-fetch": "^2.6.4",
		"@typescript-eslint/eslint-plugin": "^5.59.11",
		"@typescript-eslint/parser": "^5.59.11",
		"@vitest/coverage-v8": "^0.32.2",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"gatsby": "^5.11.0",
		"gatsby-plugin-image": "^3.11.0",
		"msw": "^1.2.2",
		"prettier": "^2.8.8",
		"prettier-plugin-jsdoc": "^0.4.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"standard-version": "^9.5.0",
		"typescript": "^5.1.3",
		"vite": "^4.3.9",
		"vite-plugin-sdk": "^0.1.1",
		"vitest": "^0.32.2"
	},
	"peerDependencies": {
		"gatsby": "^5",
		"gatsby-plugin-image": "^3"
	},
	"engines": {
		"node": ">=18"
	},
	"publishConfig": {
		"access": "public"
	},
	"gitHead": "54aa6efe0a51610af9f42d4ba55a34aaf42b7d6a"
}
