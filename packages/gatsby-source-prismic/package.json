{
	"name": "gatsby-source-prismic",
	"version": "4.1.2",
	"description": "Gatsby source plugin for building websites using Prismic as a data source",
	"keywords": [
		"gatsby",
		"gatsby-plugin",
		"gatsby-source-plugin",
		"prismic"
	],
	"homepage": "https://github.com/prismicio/prismic-gatsby/tree/main/packages/gatsby-source-prismic",
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/prismic-gatsby.git"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"sideEffects": false,
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./gatsby-node": "./dist/gatsby-node.js",
		"./package.json": "./package.json"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"src",
		"gatsby-node.cjs",
		"fragments.js"
	],
	"scripts": {
		"build": "siroc build",
		"dev": "siroc build --watch",
		"format": "prettier --write .",
		"prepare": "yarn build",
		"lint": "eslint --ext .js,.ts .",
		"unit": "nyc --reporter=lcovonly --reporter=text --exclude-after-remap=false ava",
		"test": "yarn lint && yarn unit"
	},
	"dependencies": {
		"@imgix/gatsby": "^1.6.11",
		"@prismicio/client": "^6.0.0-alpha.11",
		"@prismicio/custom-types-client": "^0.0.4",
		"@prismicio/helpers": "^2.0.0-alpha.5",
		"@prismicio/types": "^0.1.6",
		"camel-case": "^4.1.2",
		"fp-ts": "^2.10.5",
		"gatsby-node-helpers": "^1.2.1",
		"gatsby-source-filesystem": "^3.11.0",
		"tiny-hashes": "^1.0.1"
	},
	"devDependencies": {
		"@prismicio/mock": "^0.0.6",
		"@types/sinon": "^10.0.2",
		"@typescript-eslint/eslint-plugin": "^4.29.3",
		"@typescript-eslint/parser": "^4.29.3",
		"ava": "^3.15.0",
		"esbuild-register": "^3.0.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.1",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-tsdoc": "^0.2.14",
		"gatsby": "^3.12.1",
		"gatsby-image": "^3.11.0",
		"gatsby-plugin-image": "^1.11.0",
		"gatsby-plugin-utils": "^1.11.0",
		"msw": "^0.35.0",
		"nyc": "^15.1.0",
		"prettier": "^2.3.2",
		"prettier-plugin-jsdoc": "^0.3.23",
		"sinon": "^11.1.2",
		"siroc": "^0.16.0",
		"type-fest": "^2.1.0",
		"typescript": "^4.4.2"
	},
	"peerDependencies": {
		"gatsby": "^3.0.0-next.0",
		"gatsby-plugin-image": "^1.3.0-next.1"
	},
	"engines": {
		"node": ">=12.7.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
