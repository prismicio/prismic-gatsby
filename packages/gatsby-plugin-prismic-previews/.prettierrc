{
	"plugins": ["prettier-plugin-jsdoc"],
	"jsdocSeparateReturnsFromParam": true,
	"jsdocSingleLineComment": false,
	"tsdoc": true,
	"printWidth": 80,
	"useTabs": true,
	"semi": true,
	"singleQuote": false,
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"trailingComma": "all",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"arrowParens": "always",
	"requirePragma": false,
	"insertPragma": false,
	"htmlWhitespaceSensitivity": "css",
	"endOfLine": "lf"
}
